<?xml version="1.0" encoding="utf-8"?>
<com.proxy.service.widget.info.view.CsRoundLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/cs_widget_toast_parent_layout">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/toast_icon"
        style="@style/cs_widget_toast_imageview"
        tools:background="#ff0000" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/toast_content"
        style="@style/cs_widget_toast_textview"
        android:layout_marginStart="5dp"
        android:layout_toEndOf="@+id/toast_icon"
        tools:text="测试文案" />

</com.proxy.service.widget.info.view.CsRoundLayout>