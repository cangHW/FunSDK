<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <style name="cs_widget_toast_imageview">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="cs_widget_toast_parent_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#99333333</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="round_radius">8dp</item>
    </style>
    <style name="cs_widget_toast_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">#E5FFFFFF</item>
        <item name="android:textSize">20sp</item>
    </style>
    <declare-styleable name="cs_widget_RoundLayout">
        <attr format="dimension" name="round_radius" ns1:ignore="ResourceName"/>
        <attr format="dimension" name="round_radius_top_left" ns1:ignore="ResourceName"/>
        <attr format="dimension" name="round_radius_top_right" ns1:ignore="ResourceName"/>
        <attr format="dimension" name="round_radius_bottom_left" ns1:ignore="ResourceName"/>
        <attr format="dimension" name="round_radius_bottom_right" ns1:ignore="ResourceName"/>
    </declare-styleable>
</resources>